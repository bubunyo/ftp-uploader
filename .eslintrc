{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb"],
    "rules": {
      "strict": "off",
      "import/prefer-default-export": "off",
      "no-underscore-dangle": "off",
      "no-console": "off"
    }, 
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
}
